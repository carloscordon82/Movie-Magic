<h1>Edit</h1>
<form action="/user/edit" method="POST">
   <label> First Name
      <input type="text" name="firstName" value="{{firstName}}">
   </label>
   <label> Last Name
      <input type="text" name="lastName" value="{{lastName}}">
   </label>


   <button type="submit">Submit</button>
</form>

{{#if errorMessage}}
   {{errorMessage}}
{{/if}}