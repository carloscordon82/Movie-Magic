


<!-- Section 1 -->
<section class="relative py-0 bg-white lg:py-0">
 <div class="demo-wrap">
     <img
    class="demo-bg"
    src="https://image.tmdb.org/t/p/original/{{tickets.[0].movie.backdrop}}"
    alt=""
  >
  
    <div class="demo-content flex flex-col items-center justify-between px-10 mx-auto max-w-7xl xl:px-5 lg:flex-row">

        <div class="flex flex-col items-center w-full px-10 pt-5 pb-20 lg:pt-20 lg:flex-row">
           
            <div class="relative w-full max-w-md bg-cover lg:max-w-2xl lg:w-7/12">
                <div class="relative flex flex-col items-center justify-center w-full h-full lg:pr-10">
                    <img class="small-poster" src="https://image.tmdb.org/t/p/original/{{tickets.[0].movie.poster}}" style="width: 300px;">
                </div>
            </div>
 
            <div class="relative z-10 w-full max-w-2xl mt-20 lg:mt-0 lg:w-5/12">
               
                <div class="relative z-10 flex flex-col items-start justify-start p-10 bg-white shadow-2xl rounded-xl">
                    {{!-- <h4 class="w-full text-4xl font-medium leading-snug">Summary</h4> --}}
                    <h4 class="w-full text-3xl text-gray-400 font-medium leading-snug">Movie</h4>
                    <h4 class="w-full text-2xl font-medium leading-snug">{{tickets.[0].movie.title}}</h4>
                                      <h4 class="text-gray-400 w-full text-3xl font-medium leading-snug">Date</h4>
                    <h4 class="w-full text-2xl font-medium leading-snug">{{tickets.[0].date}}</h4>
                    <h4 class="w-full text-3xl text-gray-400 font-medium leading-snug">Time</h4>
                    <h4 class="w-full text-2xl font-medium leading-snug">{{tickets.[0].time}}</h4>
                    <h4 class="w-full text-3xl text-gray-400 font-medium leading-snug">Venue</h4>
                    <h4 class="w-full text-2xl font-medium leading-snug">{{tickets.[0].venue.name}}</h4>
                    <h4 class="w-full text-3xl text-gray-400 font-medium leading-snug">Seats Selected</h4>

                    <div class="relative w-full mt-1 space-y-8">
                        
                     
                            <div>
                                {{#each tickets}}

<a  class="inline-flex items-center justify-center px-4 text-base font-medium leading-6 text-white whitespace-no-wrap bg-black border border-gray-200 rounded-md shadow-sm focus:outline-none focus:shadow-none">
            {{seatNumber}}
            </a>
{{/each}}
                  
                            </div>
                

                  <form action="/checkout/process" method="POST">

<div class="relative">
                            <button type="submit" class="text-3xl inline-block w-full px-5 py-4 text-lg font-medium text-center text-white transition duration-200 bg-orange-600 rounded-lg hover:bg-orange-400 ease">Pay ${{price}}.-</button>
                            
                        </div>
                        </form>
                    </div>

                </div>
